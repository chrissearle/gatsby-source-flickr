{
  "name": "gatsby-source-flickr",
  "description": "A Gatsby source plugin to load resources from the Flickr photo search API.",
  "author": "Chris Searle <chris@chrissearle.org>",
  "repository": "https://github.com/chrissearle/gatsby-source-flickr.git",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "flickr",
    "photos"
  ],
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "format": "prettier --write \"src/**/*.js\""
  },
  "dependencies": {
    "core-js": "3.8.2",
    "node-fetch": "3.1.1",
    "query-string": "6.13.8",
    "regenerator-runtime": "0.13.7"
  },
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "cross-env": "7.0.3",
    "prettier": "2.2.1"
  },
  "version": "0.0.4"
}
